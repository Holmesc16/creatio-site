{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"browsers": "> 5%"
				},
				"modules": false,
				"useBuiltIns": "usage"
			}
		],
		"@babel/preset-react"
	],
	"plugins": [
		"babel-plugin-styled-components",
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-proposal-export-default-from",
		"@babel/plugin-syntax-dynamic-import",
		[
			"lodash",
			{
				"id": ["lodash"]
			}
		]
	]
}
